<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./head'); %>
</head>

<body>
    <div class="body-inner">
        <header id="header" data-transparent="true" data-fullwidth="true" class="<% if (!headerclass) { %>
            dark submenu-light 
        <% } else { %>
            <%= headerclass %>
        <% } %>">
            <%- include('./header'); %>
        </header>
<% if (pagetitle) { %>
    <section id="page-title" data-bg-parallax="<% if (!titlepic) { %>
        /images/parallax/5.jpg
        <% } else { %>
            <%= titlepic %> 
            <% } %> ">
        <div class="container">
        <div class="page-title">
        <h1><%= pagetitle %></h1>
        </div>
        <div class="breadcrumb">
            <ul>
                <% if (mdpath) { %>
               <%- md(mdpath + '/breadcrumb.md') %>
               <% } %>
            </ul>
            </div></div>
    </section>
    <section>
        <div class="container">
            <% if (mdpath) { %>
        <%- md(mdpath + '/index.md') %>
        <% } else { %>
        <%- md('error.md') %>
    <% } %> 
        </div>
        </section>
<% } %> 